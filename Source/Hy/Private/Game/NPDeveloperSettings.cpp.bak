#include "Game/HyDeveloperSettings.h"

UHyDeveloperSettings::UHyDeveloperSettings()
{
	bDebugDraw = false;
}

const UNPDeveloperSettings* UNPDeveloperSettings::GetDeveloperSetting()
{
	static const UNPDeveloperSettings* Settings = GetDefault<UNPDeveloperSettings>();

	return Settings;
}

UNPDeveloperSettings* UNPDeveloperSettings::GetDeveloperSettingRef()
{
	static UNPDeveloperSettings* Settings = GetMutableDefault<UNPDeveloperSettings>();

	return Settings;
}
